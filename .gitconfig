[user]
	name = Julius Plenz
	email = feh@hostname
[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	dfc = diff --cached
	lg = log -p
	tree = log --oneline --graph --decorate --all
	k = ! gitk --all
	ch = cherry -v
	b = branch -av
	i = ! git init && git commit --allow-empty -m 'initial commit' && \
		git install-myhooks
	cim = commit -m
	fm = merge --ff-only
	nfm = merge --no-ff
	install-myhooks = !~/.gitstuff/install-myhooks
	int = branch --no-merged
[color]
	branch = auto
	diff = auto
	status = auto
[merge]
	tool = vimdiff
	log = true
[rerere]
	enabled = true
[diff]
	renames = true
[myhooks]
	umlautcheck = false
